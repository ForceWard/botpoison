{
    "name": "BotPoison" ,
    "description": "Automatically trap, block, and poison bots that don't obey robots.txt rules .",
    "keywords": ["bot", "security"],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "fabpot/php-cs-fixer": "~1.7",
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Package\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    },

}

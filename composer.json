{
    "name": "mithereal/php-labrea",
    "description": "Automatically trap and block bots that don't obey robots.txt rules .",
    "keywords": ["bot", "security"],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "fabpot/php-cs-fixer": "~1.7",
        "dapphp/securimage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Package\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    },

}
